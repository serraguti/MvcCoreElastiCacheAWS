@model Coche

<h1>Details</h1>

<a asp-controller="Coches" asp-action="Index">
    Volver al listado
</a>

<div class="card" style="width: 18rem;">
    <img src="@Model.Imagen" class="card-img-top" alt="...">
    <div class="card-body">
        <h5 class="card-title">@Model.Marca @Model.Modelo</h5>
        <p class="card-text">Id Coche: @Model.IdCoche</p>
    </div>
</div>
